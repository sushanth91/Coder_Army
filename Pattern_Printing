class Main {
    public static void main(String[] args) {
        for(int row=1; row<=5; row++){
        for(int col =1; col <= 5; col++){
        System.out.print(col*col+" ");
        }
         System.out.println(" ");
        }
    }
}

7
a a a a a 
b b b b b 
c c c c c 
d d d d d 
e e e e e 

public class Main {
    public static void main(String[] args) {
        int row, col;
        for(row = 1; row <= 5; row++) {
            char name = (char) ('a' + row - 1);
            for(col = 1; col <= 5; col++) {
                System.out.print(name + " ");
            }
            System.out.println();
        }
    }
}

