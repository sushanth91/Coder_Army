a b c d e  

class Main {
    public static void main(String[] args) {
        for(int row=1; row<=5; row++){
        for(char col ='a'; col <= 'e'; col++){
        System.out.print(col+" ");
        }
         System.out.println(" ");
        }
    }
}
















